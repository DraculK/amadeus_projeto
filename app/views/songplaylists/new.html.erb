<h1>Cadastrar nova musica na Playlist<h1>

<%= form_for @songplaylist do |sp| %>
    <%= sp.label :song_id %>
    <%= sp. collection_select :song_id, Song.all, :id, :name %>
    <%= sp.label :playlist_id %>
    <%= sp.collection_select :playlist, Playlist.all, :id, :name %>
    <%= sp.submit %>
<% end %>